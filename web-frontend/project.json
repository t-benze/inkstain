{
  "name": "web-frontend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "web-frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/web-frontend",
        "index": "web-frontend/src/index.html",
        "baseHref": "/",
        "main": "web-frontend/src/main.tsx",
        "tsConfig": "web-frontend/tsconfig.app.json",
        "assets": ["web-frontend/src/favicon.ico", "web-frontend/src/assets"],
        "styles": ["web-frontend/src/styles.css"],
        "scripts": [],
        "webpackConfig": "web-frontend/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "web-frontend/src/environments/environment.ts",
              "with": "web-frontend/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "web-frontend:build",
        "hmr": true,
        "proxyConfig": "web-frontend/proxy.conf.json"
      },
      "configurations": {
        "development": {
          "buildTarget": "web-frontend:build:development"
        },
        "production": {
          "buildTarget": "web-frontend:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "web-frontend:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "web-frontend/jest.config.ts"
      }
    }
  },
  "tags": []
}
